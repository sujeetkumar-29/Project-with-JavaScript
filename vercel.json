{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/projectImg/(.*)", "destination": "/projectImg/$1" },
    { "source": "/(.*)", "destination": "/Project/$1/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*).(js|css|png|jpg|jpeg|gif|svg|ico|mp3|wav|ogg|webp)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}
